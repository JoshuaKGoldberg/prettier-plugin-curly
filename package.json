{
	"name": "prettier-plugin-curly",
	"version": "0.4.0",
	"description": "Prettier plugin to enforce consistent brace style for all control statements. ðŸ¥Œ",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/prettier-plugin-curly.git"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "commonjs",
	"exports": {
		".": {
			"import": {
				"types": "./lib/index.d.mts",
				"default": "./lib/index.mjs"
			},
			"require": {
				"types": "./lib/index.d.ts",
				"default": "./lib/index.js"
			}
		}
	},
	"main": "lib/index.js",
	"files": [
		"lib/"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier . --write",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@babel/types": "7.28.4",
		"@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
		"@eslint/js": "9.38.0",
		"@release-it/conventional-changelog": "10.0.1",
		"@types/eslint-plugin-markdown": "2.0.2",
		"@types/node": "24.9.1",
		"@vitest/coverage-v8": "4.0.1",
		"@vitest/eslint-plugin": "1.3.23",
		"console-fail-test": "0.5.0",
		"create-typescript-app": "2.46.3",
		"cspell": "9.2.2",
		"eslint": "9.38.0",
		"eslint-plugin-jsdoc": "61.1.5",
		"eslint-plugin-jsonc": "2.21.0",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-n": "17.23.1",
		"eslint-plugin-package-json": "0.58.0",
		"eslint-plugin-perfectionist": "4.15.1",
		"eslint-plugin-regexp": "2.10.0",
		"eslint-plugin-yml": "1.19.0",
		"husky": "9.1.7",
		"knip": "5.66.2",
		"lint-staged": "16.2.6",
		"markdownlint": "0.39.0",
		"markdownlint-cli": "0.45.0",
		"prettier": "3.6.2",
		"prettier-plugin-packagejson": "2.5.19",
		"prettier-plugin-sh": "0.18.0",
		"release-it": "19.0.5",
		"sentences-per-line": "0.3.0",
		"tsup": "8.5.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.46.2",
		"vitest": "4.0.1"
	},
	"peerDependencies": {
		"prettier": "^2 || ^3"
	},
	"packageManager": "pnpm@10.19.0",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
